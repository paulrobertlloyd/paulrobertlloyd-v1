<mt:If tag="AssetCount">
	<mt:Assets type="image" lastn="10">
	<mt:AssetsHeader>
	<h3>Recent Assets</h3>
	<ul>
	</mt:AssetsHeader>
		<li><a class="asset-image" href="<mt:AssetURL/>"><img src="<mt:AssetThumbnailURL height="70"/>" class="photo" alt="<mt:AssetLabel/>" title="<mt:AssetLabel/>"/></a></li>
	<mt:AssetsFooter></ul></mt:AssetsFooter>
	</mt:Assets>
</mt:If>